# Snowflake Connection Configuration
# Copy this file to .env and fill in your actual values
# Never commit .env to version control!

# Snowflake Account (e.g., your-org.eu-west-1 or xy12345)
SNOWFLAKE_ACCOUNT=your-account-here

# Snowflake Username
SNOWFLAKE_USER=your-nhs-email

# Snowflake Role (Code requires at least ENGINEER)
SNOWFLAKE_ROLE=ENGINEER

# Snowflake Warehouse (e.g., NCL_ANALYTICS_XS)
SNOWFLAKE_WAREHOUSE=your-warehouse-here

# One Drive Configurations
# Folder name for the team OneDrive root
NHS_ONEDRIVE_DIR = "OneDrive - NHS"
# Parent folder for the team SharePoint
WORK_DIR = "(SCB) Centre for Cancer Outcomes - Documents"
# Child folder name on SharePoint where the data files are saved
DATA_SOURCE_DIR = "COSD HTML"

# Destination Settings
DESTINATION_DATABASE = "DATA_LAKE__NCL"
DESTINATION_SCHEMA = "CANCER__COSD_HTML"